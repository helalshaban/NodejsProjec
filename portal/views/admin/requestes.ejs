    <section>
        <table>

            <tr>
                <th>Type</th>
                <th>Request</th>
                <th>Username</th>
                <th>Action</th>
            </tr>

            <% if (data.length != 0) {for (var i = 0; i < data.length ; i++){ %>

                <tr>
                    <td>
                        <%= data[i].Type %>
                    </td>
                    <td>
                        <%= data[i].request %>
                    </td>
                    <td>
                        <%= data[i].Username %>
                    </td>

                    <td>
                        <% if (data[i].Type != 'Other'){ %>
                            <form method="post" action="/managerstdreq/adminAcpeption">
                                <input type="text" name="requesth" value="<%= data[i].request %>" hidden>
                                <input type="text" name="Usernameh" value="<%= data[i].Username %>" hidden>
                                <input type="text" name="Type" value="<%= data[i].Type %>" hidden>
                                <input type="submit" name="submit" value="Accept" style="margin:30px">
                            </form>
                            <% } %>
                                <form method="post" action="/managerstdreq/admindeleterequest">
                                    <input type="text" name="request" value="<%= data[i].request %>" hidden>
                                    <input type="text" name="Username" value="<%= data[i].Username %>" hidden>
                                    <input type="submit" name="submit" value="reject" style="margin:30px">
                                </form>
                    </td>
                </tr>
                <% }} %>
        </table>
